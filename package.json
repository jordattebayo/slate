    "www-drop-database": "NODE_TLS_REJECT_UNAUTHORIZED=0 node ./scripts drop-database"
  },
  "build": {
    "appId": "com.slate",
    "productName": "slate",
    "mac": {
      "icon": " build/icon.icns",
      "asar": "false"
    },
    "extraMetadata": {
      "main": "./electron/main.js"
    }
